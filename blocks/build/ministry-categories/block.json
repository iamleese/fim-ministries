{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "fim-ministries/ministry-categories",
  "version": "0.1.0",
  "title": "Ministry Categories",
  "category": "ministry-blocks",
  "description": "Lists ministry categories",
  "supports": {
    "html": true
  },
  "attributes": {
    "show_subcategories": {
      "type": "boolean",
      "default": true
    },
    "hide_empty": {
      "type": "boolean",
      "default": false
    },
    "show_all": {
      "type": "boolean",
      "default": true
    },
    "show_all_text": {
      "type": "string",
      "default": "All Ministries"
    }
  },
  "textdomain": "fim-ministries",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}